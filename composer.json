{
    "name": "larangular/file-manager",
    "description": "laravel file upload with db record",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0",
        "larangular/support": "^0.1.0|^0.2.0",
        "larangular/routing-controller": "^0.1.0|^0.2.0",
        "larangular/migration-package": "^0.1.0|^0.2.0",
        "larangular/installable": "^0.1.0|^0.2.0",
        "jedrzej/searchable": "^0.0.14"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\FileManager\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larangular\\FileManager\\Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\FileManager\\FileManagerServiceProvider"
            ],
            "aliases": {
                "FileManager": "Larangular\\FileManager\\Facades\\FileManagerController"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.7"
    }
}
